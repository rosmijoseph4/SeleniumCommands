<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression Suite">
    <test name="Functionaltest" parallel="methods" thread-count="2">
        <parameter name="browser" value="chrome"/>
        <parameter name="url" value="http://demowebshop.tricentis.com/"/>
        <classes>
            <class name="com.obs.seleniumbasics.SeleniumCommands">
                <methods>
                    <include name="verifyHomePageTitle"/>
                    <include name="VerifyWindowHandle"/>
                    <exclude name="VerifyFileUpload"/>
                    <include name="verifyUrl"/>
                    <exclude name="verifyFileuploadRobot"/>
                    <exclude name="verifyJavascriptClickAndSendkeys"/>
                    <include name="verifyValidloginExcel"/>
                    <exclude name="verifyLoginUsingDataProvider"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>